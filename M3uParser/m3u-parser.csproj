<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PackageId>m3u-parser</PackageId>
    <Title>M3U Parser</Title>
    <PackageDescription>Package for parsing m3u playlist</PackageDescription>
    <Version>1.0.1</Version>
    <Authors>Karolis Arbaciauskas</Authors>
    <Company>Charlie on Acid</Company>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>m3u_parser</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <repository type="git" url="https://github.com/Acidfabric/m3u-parser"/>
    <PackageReleaseNotes>Version 1.0.1 - Added attributes: tvg-id, tvg-name, group-title, parent-code, tvg-logo, tvg-logo-small.</PackageReleaseNotes>
  </PropertyGroup>
</Project>
